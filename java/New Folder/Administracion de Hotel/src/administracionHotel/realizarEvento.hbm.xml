<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  
 <hibernate-mapping>  
  <class name="administracion.model.RealizarEvento" table="REALIZAREVENTO">  
    
    <id name="eventoId">  
        <generator class="native"></generator>  
    </id>  
    
    <property name="tipoEvento"></property>  
    <property name="fecha"></property>  
    <property name="Duracion"></property>  
    <property name="costoEvento"></property>  
     
  </class>        
 </hibernate-mapping> 