 <?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">   
 <hibernate-mapping>  
  <class name="administracionHotel.model.Reservacion" table="RESERVACION">  
    
    <id name="reservacionId">  
        <generator class="native"></generator>  
    </id>  
    
    <property name="numeroHuespedes"></property>  
    <property name="precioServicio"></property>  
    <property name="precioReservacionHabitacion"></property>  
    <property name="precioReservacionComida"></property>  
    <property name="tiempoEstancia"></property>  
    <property name="descuento"></property>  
    <property name="precioReservacion"></property>
    
     
  </class>        
 </hibernate-mapping> 
