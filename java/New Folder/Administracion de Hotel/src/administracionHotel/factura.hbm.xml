<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  
 <hibernate-mapping>  
  <class name="administracion.model.Factura" table="FACTURA">  
    
    <id name="numero">  
        <generator class="native"></generator>  
    </id>  
    
    <property name="fecha"></property>
    <property name="Ruc"></property>   
    <property name="valorSubtotal"></property>
    <property name="valorTotal"></property>
    <property name="valorIva"></property>  
     
     
  </class>        
 </hibernate-mapping>
